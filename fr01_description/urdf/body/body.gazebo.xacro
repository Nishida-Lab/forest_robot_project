<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">  
  <xacro:macro name="base_link_gazebo_v0">
	<gazebo reference="base_link">
	  <selfCollide>true</selfCollide>
      <mu1 value="0.8" />
      <mu2 value="0.8" />
    </gazebo>
  </xacro:macro>

  <xacro:macro name="base_stabilized_gazebo_v0">
	<gazebo reference="base_stabilized">
	  <selfCollide>false</selfCollide>
      <mu1 value="0.8" />
      <mu2 value="0.8" />
    </gazebo>
  </xacro:macro>
</robot>
